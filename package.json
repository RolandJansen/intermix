{
  "name": "intermix.js",
  "version": "1.0.0",
  "description": "Javascript library for sound/audio-processing",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "lint": "jshint src/",
    "build": "npm run build:js",
    "watch": "npm run watch:js",
    "test": "jasmine-node tests/specs/",
    "clean": "rimraf dist",
    "build:js": "browserify index.js -d -v -p [minifyify --no-map]> dist/intermix.min.js",
    "prebuild:js": "npm run lint",
    "watch:js": "watchify index.js -o dist/intermix.js -d -v -p [minifyify --map dist/intermix.map.json --output dist/intermix.map.json]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RolandJansen/intermix.js.git"
  },
  "keywords": [
    "intermix.js",
    "WebAudio",
    "sequencer",
    "sequencing",
    "synthesizer",
    "synthesis",
    "sound",
    "audio",
    "music"
  ],
  "author": "Roland Jansen",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/RolandJansen/intermix.js/issues"
  },
  "homepage": "https://github.com/RolandJansen/intermix.js#readme",
  "devDependencies": {
    "browserify": "^13.0.0",
    "jasmine-core": "^2.4.1",
    "jasmine-node": "^1.14.5",
    "jshint": "^2.9.1",
    "karma-jasmine": "^0.3.6",
    "minifyify": "^7.1.0",
    "rimraf": "^2.5.0",
    "watchify": "^3.7.0"
  }
}
