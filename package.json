{
  "name": "intermix",
  "version": "0.6.0",
  "description": "Javascript/Typescript framework for building music apps",
  "author": "Roland Jansen",
  "license": "LGPL-3.0-or-later",
  "files": ["dist"],
  "main": "dist/intermix.js",
  "types": "dist/intermix.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "eslint src/**/*.{js,ts}",
    "prettify": "eslint src/**/*.{js,ts} --fix",
    "clean": "rimraf dist/* && rimraf build/*",
    "doc": "typedoc",
    "prebuild": "npm run lint && npm run clean",
    "build_old": "webpack --config webpack.prod.js",
    "build": "rollup -c",
    "compile": "tsc",
    "watch": "webpack-dev-server --config webpack.dev.js",
    "watch-test": "jest --watch",
    "test": "jest --coverage",
    "test:all": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RolandJansen/intermix.git"
  },
  "bugs": {
    "url": "https://github.com/RolandJansen/intermix/issues"
  },
  "homepage": "https://rolandjansen.github.io/intermix/",
  "keywords": [
    "intermix.js",
    "sound",
    "audio",
    "web audio",
    "sequencer",
    "sequencing",
    "synthesizer",
    "synthesis",
    "music"
  ],
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testURL": "http://localhost/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/jest": "^23.3.1",
    "@types/node": "^12.12.14",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "@wessberg/rollup-plugin-ts": "^1.3.2",
    "atob": "^2.1.2",
    "benchmark": "^2.1.4",
    "dts-bundle-webpack": "^1.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "expect-more-jest": "^4.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-externals-plugin": "^3.8.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "jest-extended": "^0.11.0",
    "lint-staged": "^10.2.7",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.3",
    "rollup-plugin-web-worker-loader": "^1.3.1",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "^4.1.0",
    "ts-jest": "^25.0.0",
    "ts-loader": "^7.0.0",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3",
    "web-audio-test-api": "^0.5.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "redux": "^4.0.5"
  }
}
