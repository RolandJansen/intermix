{
  "name": "intermix",
  "version": "0.3.0",
  "description": "Javascript library for building music apps",
  "main": "dist/intermix.js",
  "types": "dist/intermix.d.ts",
  "scripts": {
    "lint": "tslint src/main.ts",
    "clean": "rimraf dist/*",
    "docgen": "jsdoc -c conf.json --verbose",
    "docgen:md": "jsdoc2md src/**/*.js > doc/api.md",
    "prebuild": "npm run lint && npm run clean",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "test:cmd": "npm run test:node:cmd",
    "test:node": "istanbul cover --include-all-sources jasmine JASMINE_CONFIG_PATH=test/jasmine.json",
    "test:node:cmd": "istanbul cover --include-all-sources node_modules/jasmine/bin/jasmine.js JASMINE_CONFIG_PATH=test/jasmine.json",
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RolandJansen/intermix.js.git"
  },
  "keywords": [
    "intermix.js",
    "sound",
    "audio",
    "web audio",
    "sequencer",
    "sequencing",
    "synthesizer",
    "synthesis",
    "music"
  ],
  "author": "Roland Jansen",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/RolandJansen/intermix.js/issues"
  },
  "homepage": "https://github.com/RolandJansen/intermix.js#readme",
  "eslintConfig": {
    "env": {
      "es6": true
    }
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testURL": "http://localhost/"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "atob": "^2.0.3",
    "expect-more-jest": "^2.3.0",
    "jest": "^23.4.2",
    "jest-extended": "^0.11.0",
    "jsdoc": "^3.4.0",
    "jsdoc-to-markdown": "^1.3.6",
    "rimraf": "^2.5.0",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^23.0.1",
    "tslint": "^5.10.0",
    "typescript": "^2.8.1",
    "web-audio-test-api": "^0.5.2"
  },
  "dependencies": {
    "redux": "^4.0.0"
  }
}
