{
  "name": "intermix.js",
  "version": "1.0.0",
  "description": "Javascript library for sound/audio-processing",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "lint": "eslint src/",
    "build": "npm run build:js && npm run build:minijs",
    "watch": "npm run watch:js",
    "test": "jasmine-node tests/specs/",
    "clean": "rimraf dist/*",
    "build:js": "browserify index.js -o dist/intermix.js -v -p browserify-derequire -s Intermix",
    "build:minijs": "browserify index.js -o dist/intermix.min.js -d -v -p browserify-derequire -p [minifyify --map dist/intermix.map.json --output dist/intermix.map.json]",
    "prebuild:js": "npm run clean && npm run lint",
    "watch:jswatchify": "watchify index.js -o dist/intermix.min.js -d -v -p [minifyify --map dist/intermix.map.json --output dist/intermix.map.json]",
    "watch:js": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RolandJansen/intermix.js.git"
  },
  "keywords": [
    "intermix.js",
    "WebAudio",
    "sequencer",
    "sequencing",
    "synthesizer",
    "synthesis",
    "sound",
    "audio",
    "music"
  ],
  "author": "Roland Jansen",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/RolandJansen/intermix.js/issues"
  },
  "homepage": "https://github.com/RolandJansen/intermix.js#readme",
  "devDependencies": {
    "browserify": "^13.0.0",
    "browserify-derequire": "^0.9.4",
    "eslint": "^1.10.3",
    "eslintify": "^1.1.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-browserify": "^4.4.2",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jasmine": "^0.3.6",
    "minifyify": "^7.1.0",
    "rimraf": "^2.5.0",
    "watchify": "^3.7.0"
  }
}
