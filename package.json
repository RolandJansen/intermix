{
  "name": "intermix",
  "version": "0.4.0",
  "description": "Javascript library for building music apps",
  "main": "dist/intermix.js",
  "types": "dist/intermix.d.ts",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "clean": "rimraf dist/*",
    "doc": "typedoc",
    "prebuild": "npm run lint && npm run clean",
    "build": "webpack --config webpack.prod.js",
    "compile": "tsc",
    "watch": "webpack-dev-server --config webpack.dev.js",
    "watch-test": "jest --watch",
    "test": "jest --coverage",
    "test:all": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RolandJansen/intermix.js.git"
  },
  "keywords": [
    "intermix.js",
    "sound",
    "audio",
    "web audio",
    "sequencer",
    "sequencing",
    "synthesizer",
    "synthesis",
    "music"
  ],
  "author": "Roland Jansen",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/RolandJansen/intermix.js/issues"
  },
  "homepage": "https://github.com/RolandJansen/intermix.js#readme",
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testURL": "http://localhost/"
  },
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/jest": "^23.3.1",
    "@types/node": "^12.12.14",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "atob": "^2.1.2",
    "benchmark": "^2.1.4",
    "dts-bundle-webpack": "^1.0.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "expect-more-jest": "^4.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-externals-plugin": "^3.8.0",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.3.0",
    "jest-extended": "^0.11.0",
    "rimraf": "^3.0.2",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^25.0.0",
    "ts-loader": "^7.0.0",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3",
    "web-audio-test-api": "^0.5.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "redux": "^4.0.5"
  }
}
